{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "default",
  "description": "Default capabilities for the application",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "shell:default",
    "dialog:default",
    "dialog:allow-open",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "node",
          "cmd": "/Users/wsuam/.opencode/node/darwin-arm64/bin/node",
          "args": true,
          "sidecar": false
        }
      ]
    },
    {
      "identifier": "shell:allow-spawn",
      "allow": [
        {
          "name": "node",
          "cmd": "/Users/wsuam/.opencode/node/darwin-arm64/bin/node",
          "args": true,
          "sidecar": false
        }
      ]
    }
  ]
}
